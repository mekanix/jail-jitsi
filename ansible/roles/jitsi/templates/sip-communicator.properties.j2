# The videobridge uses 443 by default with 4443 as a fallback, but since we're already
# running nginx on 443 in this example doc, we specify 4443 manually to avoid a race condition
org.jitsi.videobridge.TCP_HARVESTER_PORT=4443
org.ice4j.ice.harvest.NAT_HARVESTER_LOCAL_ADDRESS={{ hostvars[inventory_hostname]['ansible_default_ipv4']['address'] }}
org.ice4j.ice.harvest.NAT_HARVESTER_PUBLIC_ADDRESS={{ ipify_public_ip }}

#
# For Grafana dashboard
#

# the callstats credentials
io.callstats.sdk.CallStats.appId="tildastats"
io.callstats.sdk.CallStats.keyId="GRAFANAKEY"
